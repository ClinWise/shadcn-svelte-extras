{
	"$schema": "https://unpkg.com/jsrepo@1.21.0/schemas/registry-config.json",
	"dirs": ["./src/lib/components", "./src/lib"],
	"includeBlocks": [],
	"includeCategories": ["ui", "hooks", "utils", "actions", "styles"],
	"excludeBlocks": [],
	"excludeCategories": [],
	"doNotListBlocks": [],
	"doNotListCategories": [],
	"listBlocks": [
		"button",
		"chat",
		"code",
		"field-set",
		"ipv4address-input",
		"kbd",
		"light-switch",
		"modal",
		"phone-input",
		"snippet",
		"tags-input",
		"theme-selector",
		"tree-view",
		"active.svelte",
		"shortcut.svelte",
		"context-provider",
		"use-auto-scroll.svelte"
	],
	"listCategories": [],
	"excludeDeps": ["svelte"],
	"preview": true,
	"rules": {
		"no-category-index-file-dependency": "warn",
		"no-unpinned-dependency": "warn",
		"require-local-dependency-exists": "error",
		"max-local-dependencies": ["warn", 10],
		"no-circular-dependency": "error",
		"no-unused-block": "warn",
		"no-framework-dependency": "error"
	}
}
